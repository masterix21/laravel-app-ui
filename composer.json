{
    "name": "masterix21/laravel-app-ui",
    "description": "Laravel APP UI components",
    "keywords": [
        "masterix21",
        "laravel",
        "app-ui"
    ],
    "homepage": "https://github.com/masterix21/laravel-app-ui",
    "license": "MIT",
    "authors": [
        {
            "name": "Luca Longo",
            "email": "l.longo@ambita.it",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/contracts": "^8.37",
        "spatie/laravel-package-tools": "^1.4.3",
        "thomaswelton/laravel-gravatar": "^1.3"
    },
    "require-dev": {
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "pestphp/pest": "^1.18",
        "pestphp/pest-plugin-laravel": "^1.1",
        "spatie/laravel-ray": "^1.23",
        "vimeo/psalm": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "Masterix21\\AppUI\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Masterix21\\AppUI\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "./vendor/bin/pest --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Masterix21\\AppUI\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
